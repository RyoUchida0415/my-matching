<%= render 'users/header' %>

<div class="col-md-3">
	<%= render 'users/sidebar', user: current_user %>
</div>

<div class="col-md-9">
	<h2>マッチング成立</h2>
	<!-- マッチングが成立しているならば -->
	<% if @users.count > 0 %>
	<%= render '/users/index', users: @users %>
	<!-- マッチングが成立していないならば -->
	<% else %>
	<p>ユーザーはいません</p>
	<% end %>
</div>

